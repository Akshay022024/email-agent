{
  "version": "2.0",
  "languageWorkers": {
    "python": {
      "defaultExecutablePath": "python",
      "defaultWorkerPath": "azure_functions_worker/main.py"
    }
  },
  "functionTimeout": "00:05:00",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  }
}
